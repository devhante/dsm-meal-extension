{
    "manifest_version" : 2,
    
    "name" : "DSM Meal Extension",
    "description" : "Chrome meal extension program for DSM students",
    "version" : "1.0.3",
    "author" : "Ethan Cho",
    
    "browser_action" : {
        "default_icon" : "icon128.png",
        "default_popup" : "popup.html"
    },
    
    "icons" : {
        "16" : "icon16.png",
        "24" : "icon24.png",
        "32" : "icon32.png",
        "64" : "icon64.png",
        "128" : "icon128.png"
    },
    
    "permissions" : [
        "tabs",
        "<all_urls>"
    ]
}