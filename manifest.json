{
    "manifest_version" : 2,
    
    "name" : "DSM Meal Extension",
    "description" : "Chrome meal extension program for DSM students",
    "version" : "1.3",
    "author" : "Ethan Cho",
    
    "browser_action" : {
        "default_icon" : "static/icons/icon128.png",
        "default_popup" : "html/popup.html"
    },
    
    "icons" : {
        "16" : "static/icons/icon16.png",
        "24" : "static/icons/icon24.png",
        "32" : "static/icons/icon32.png",
        "64" : "static/icons/icon64.png",
        "128" : "static/icons/icon128.png"
    },
    
    "permissions" : [
        "tabs",
        "<all_urls>",
        "storage"
    ]
}